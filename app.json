{
  "expo": {
    "name": "Jetset",
    "slug": "jetset",
    "version": "1.0.0",
    "orientation": "portrait",
    "sdkVersion": "52.0.0",
    "splash": {
      "image": "./src/assets/splash/splash-icon-light.png",
      "resizeMode": "contain",
      "backgroundColor": "#3BACE3",
      "hideExponentText": true
    },
    "notification": {
      "icon": "./src/assets/icons/Light.png",
      "color": "#3BACE3",
      "iosDisplayInForeground": true
    },
    "ios": {
      "supportsTablet": true,
      "usesAppleSignIn": true,
      "bundleIdentifier": "com.kpegra1.jetset",
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "UIBackgroundModes": [
          "remote-notification"
        ]
      },
      "icon": "./src/assets/icons/Light.png",
      "alternateIcons": {
        "dark": "./src/assets/icons/Dark.png",
        "light": "./src/assets/icons/Light.png",
        "tinted": "./src/assets/icons/Tinted.png"
      }
    },
    "android": {
      "newArchEnabled": false,
      "package": "com.pegram.jetset",
      "googleServicesFile": "./google-services.json",
      "buildToolsVersion": "35.0.0",
      "compileSdkVersion": 34,
      "targetSdkVersion": 34,
      "minSdkVersion": 24,
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/icons/adaptive-icon.png",
        "monochromeImage": "./src/assets/icons/adaptive-icon.png",
        "backgroundColor": "#ffffff",
        "dark": {
          "foregroundImage": "./src/assets/icons/adaptive-icon-dark.png",
          "monochromeImage": "./src/assets/icons/adaptive-icon-dark.png",
          "backgroundColor": "#000000"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-apple-authentication",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static",
            "deploymentTarget": "15.1"
          }
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./src/assets/icons/Light.png",
          "color": "#3BACE3"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "cf7b3e9e-07c0-457e-b3b0-d4179b54b637"
      }
    },
    "scheme": "jetset",
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/cf7b3e9e-07c0-457e-b3b0-d4179b54b637"
    }
  }
}
